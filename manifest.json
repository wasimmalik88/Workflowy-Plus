{
  "manifest_version": 2,
  "name": "WorkFlowy Plus",
  "version": "1.1.6",
  "description": "Get most out of WorkFlowy",
  "browser_action": {
    "default_icon": "tick.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["eventpage.js"],
    "persistent": false
  },
  "content_scripts": [{
      "css": ["style.css"],
      "js": ["jquery-1.8.3.min.js", "content.js"],
      "matches": ["*://*.workflowy.com/*"]
    }

  ],
  "permissions": [
    "contextMenus",
    "storage",
    "https://sadds213.000webhostapp.com/"
  ],
  "icons": {
    "16": "ok16.png"
  }
}